# MySQL Secret 예시
# 실제 Secret 생성 방법:
# kubectl create secret generic mysql-secrets \
#   --from-literal=root-password='your-root-password' \
#   --from-literal=username='dash_user' \
#   --from-literal=password='your-user-password' \
#   --namespace=dash

apiVersion: v1
kind: Secret
metadata:
  name: mysql-secrets
  namespace: dash
type: Opaque
stringData:
  root-password: "change-me"
  username: "dash_user"
  password: "change-me"

